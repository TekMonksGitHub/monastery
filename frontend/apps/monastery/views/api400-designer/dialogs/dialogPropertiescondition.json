{
    "showOKButton": false,
    "showCancelButton": false,
    "modal": false,
    "okIcon": "./views/{{url.view}}/dialogs/ok.svg",
    "closeIcon": "./views/{{url.view}}/dialogs/close.svg",
    "showOKIcon": true,
    "showCancelIcon": true,
    "showErrorNotifier": false,

   
    "var--dialog-font": "Raleway",
    "var--dialog-background": "#DFF0FE",
    "var--dialog-left": "210px",
    "var--dialog-width": "calc(100% - 420px)",
    "var--animation-name": "flyinbottom",
    "var--animation-duration": "0.18s",
    "var--animation-left-transform": "0%",
    "var--button-background-color": "#B6DCFE",
    "var--button-text-color": "black",
    "var--margin-bottom": "0px",
    "var--error-display": "block",
    "var--dialog-position": "absolute",
    "var--box-shadow": "0px 10px 0px rgba(0,0,0,0.19), 0 6px 6px rgba(0,0,0,0.23);",
    "var--dialog-border": "1px solid #98CCFD",
    "var--dialog-border-radius": "20px 20px 0px 0px ",
    "var--dialog-max-height": "calc(100vh )"
}